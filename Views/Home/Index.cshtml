@model homeModel

@{
    ViewData["Title"] = "Finance Bot"; // Set your page title here
}

<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link
        href="https://fonts.googleapis.com/css2?family=Josefin+Sans:wght@200;300;400;500&family=Raleway:wght@100;200;300;400;500&family=Roboto:wght@300;400;700&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css"
        integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/css/home.css" />

</head>

<body >
    <!-- Your view content goes here -->
    <span class="desktop">
        <header>
            <nav>
                <a href="#">Home</a>
                <a href="#">About</a>
            </nav>
        </header>
  </span>

  <!--Mobile Navigation-->
  <span class="mobile">
    <header>
      <div class="button_container">
        <span class="top"></span>
        <span class="middle"></span>
        <span class="bottom"></span>
      </div>
    </header>

    <div class="overlay">
      <nav>
        <ul>
          <li><a href="#">Home</a></li>
          <li><a href="#">About</a></li>
        </ul>
      </nav>
    </div>
  </span>
    <sectio class="nav">
        <h1 id="bot-name">@Model.BotName</h1>

        <h3 class="span loader" id = "slogan">
            @foreach (var letter in Model.Slogan)
            {
                 @if (@letter == ' ')
                {
                    <span> &nbsp;</span>
                }
                else
                {
                    <span class="m">@letter</span>
                }
            }
            <br>
            <br>
            <form action="/wealthpixChat/Intro">
                <div class="buttonwrap">
                    <button type="submit" class="button">Click to start</button>
                </div>
            </form>

        </h3>
        <br>
        <div class="wrapper1">

            <div id="date"></div>

        </div>

    </sectio>
    <main class="main">

    </main>
    <div id="particles-js" class ="background">    </div>



</body>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script src="js/jquery-2.2.3.min.js"></script>
<script src="https://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js"></script>
<script src="~/js/home.js"></script>